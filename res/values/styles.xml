    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
    </style>

    <!-- CirclePageIndicator theme. -->
    <style name="Widget"></style>

</resources>
